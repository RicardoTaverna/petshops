<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CadastrarClienteController</servlet-name>
        <servlet-class>Controller.CadastrarClienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarFornecedorController</servlet-name>
        <servlet-class>Controller.CadastrarFornecedorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarFornecedorIndexController</servlet-name>
        <servlet-class>Controller.ListarFornecedorIndexController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginClienteController</servlet-name>
        <servlet-class>Controller.LoginClienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dashboard</servlet-name>
        <servlet-class>Controller.DashboardClienteController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastrarClienteController</servlet-name>
        <url-pattern>/CadastrarClienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarFornecedorController</servlet-name>
        <url-pattern>/CadastrarFornecedorController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarFornecedorIndexController</servlet-name>
        <url-pattern>/ListarFornecedorIndexController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginClienteController</servlet-name>
        <url-pattern>/LoginClienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>ListarFornecedorIndexController</welcome-file>
    </welcome-file-list>
</web-app>
